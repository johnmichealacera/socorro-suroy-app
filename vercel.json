{
  "functions": {
    "api/*.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.php" },
    { "src": "/api/(.*)", "dest": "/api/about.php" },
    { "src": "/api/(.*)", "dest": "/api/contact.php" },
    { "src": "/css/style.css", "dest": "/css/style.css" },
    { "src": "/js/main.js", "dest": "/js/main.js" }
  ]
}
